 '(custom-enabled-themes '(wombat))
 '(display-line-numbers-type 'relative)
 '(global-display-line-numbers-mode t)
 '(package-selected-packages '(company evil key-chord org-roam)))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(default ((t (:family "Courier Prime" :foundry "QUQA" :slant normal :weight regular :height 113 :width normal))))
 '(org-code ((t (:inherit shadow :box (:line-width (1 . 1) :color "black" :style released-button)))))
 '(org-level-1 ((t (:inherit outline-1 :extend nil :underline t :height 1.3))))
 '(org-level-2 ((t (:inherit outline-2 :extend nil :underline t :height 1.2))))
 '(org-level-3 ((t (:inherit outline-3 :extend nil :underline t :height 1.2))))
 '(org-level-4 ((t (:inherit outline-4 :extend nil :underline t :height 1.15))))
 '(org-link ((t (:inherit link :weight bold :height 1.17)))))

